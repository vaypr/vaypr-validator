{
  "rules": {
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-before-instance",
          "variables-before-functions"
        ]
      }
    ],
    "no-inferrable-types": true,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "curly": true,
    "forin": true,
    "label-position": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace",
      "warn",
      "error"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": [true, {"check-parameters": true}],
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "radix": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "cyclomatic-complexity": true,
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [
      true,
      140
    ],
    "object-literal-sort-keys": false,
    "class-name": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": true,
    "variable-name": false,
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}